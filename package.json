{
  "name": "char-count-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run generate-types && next build",
    "start": "next start",
    "lint": "next lint",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "generate-types": "node --loader ts-node/esm src/lib/i18n/type-generator.ts assets/locales AllMessages src/lib/i18n/generated-types.ts",
    "i18n-validate": "node --loader ts-node/esm src/scripts/validate-i18n.ts",
    "i18n-report": "node --loader ts-node/esm src/scripts/i18n-report.ts",
    "i18n-fix": "node --loader ts-node/esm src/scripts/i18n-fix-helper.ts",
    "i18n-check": "node --loader ts-node/esm src/scripts/check-i18n-keys.ts",
    "i18n": "npm run i18n-validate && npm run i18n-fix ja && npm run i18n-fix es && npm run i18n-fix ru && npm run i18n-fix zh && npm run i18n-report && npm run i18n-check",
    "precommit": "npm run i18n",
    "prepare": "husky"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.6.0",
    "@heroicons/react": "^2.2.0",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "^15.1.6",
    "@sparticuz/chromium-min": "^133.0.0",
    "@types/cheerio": "^0.22.35",
    "@types/mdx": "^2.0.13",
    "axios": "^1.7.9",
    "chalk": "^5.4.1",
    "cheerio": "^1.0.0",
    "cookies-next": "^5.1.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.5.0",
    "gray-matter": "^4.0.3",
    "heic-convert": "^2.1.0",
    "jspdf": "^3.0.0",
    "jszip": "^3.10.1",
    "negotiator": "^1.0.0",
    "next": "15.1.6",
    "next-mdx-remote": "^5.0.0",
    "next-sitemap": "^4.2.3",
    "pdf-lib": "^1.17.1",
    "puppeteer-core": "^24.3.0",
    "qrcode.react": "^4.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-pdf": "^9.2.1",
    "remark-gfm": "^4.0.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/typography": "^0.5.16",
    "@types/file-saver": "^2.0.7",
    "@types/heic-convert": "^2.1.0",
    "@types/negotiator": "^0.6.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "husky": "^9.1.7",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "type": "module"
}
